[core]
  editor = vim
[alias]
	cached = diff --cached
	st = status -s
	graph = show-branch
	last = log -1
	past = log -1 -p
	line = log --oneline -3
	liness = log --oneline
	lines = log --oneline -6
	stashed = stash list
	cfg = config --global
	append = add -v
	vu = add -vu
	amend = commit --amend
